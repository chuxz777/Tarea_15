@model IEnumerable<WebApplication1.Models.survey_results>

@{
    ViewBag.Title = "Index";
}

<h2>Survey Results</h2>

<table class="table">
    <tr>
        <th>
            Total of surveys answered
        </th>
        <th>
            Total of registered users
        </th>
        <th>
            Stadistic last refresh
        </th>
        <th>
            % Completed
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.total_surveys)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.total_users)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.stat_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.percentage_completed) %
        </td>

    </tr>
}

</table>
